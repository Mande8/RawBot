include "m8c.inc"


;--------------------------------------------------
; Constants for Timer8Main API's.
;--------------------------------------------------

Timer8Main_CONTROL_REG_START_BIT:          equ 0x01    ; Control register start bit
Timer8Main_INT_REG:                        equ 0x0e1
Timer8Main_INT_MASK:                       equ 0x01


;--------------------------------------------------
; Constants for Timer8Main user defined values
;--------------------------------------------------

Timer8Main_PERIOD:                         equ 0xc7
Timer8Main_COMPARE_VALUE:                  equ 0x00


;--------------------------------------------------
; Register Address Constants for Timer8Main
;--------------------------------------------------

Timer8Main_COUNTER_REG:                equ 0x20   ; DR0 Counter register
Timer8Main_PERIOD_REG:                 equ 0x21   ; DR1 Period register
Timer8Main_COMPARE_REG:                equ 0x22   ; DR2 CompareValue register
Timer8Main_CONTROL_REG:                equ 0x23   ; Control register
Timer8Main_FUNC_REG:                   equ 0x20   ; Function register
Timer8Main_INPUT_REG:                  equ 0x21   ; Input register
Timer8Main_OUTPUT_REG:                 equ 0x22   ; Output register


;--------------------------------------------------
; Timer8Main Macro 'Functions'
;--------------------------------------------------

   macro Timer8Main_Start_M
   or    reg[Timer8Main_CONTROL_REG],  Timer8Main_CONTROL_REG_START_BIT
   endm

   macro Timer8Main_Stop_M
   and   reg[Timer8Main_CONTROL_REG], ~Timer8Main_CONTROL_REG_START_BIT
   endm

   macro Timer8Main_EnableInt_M
   M8C_EnableIntMask Timer8Main_INT_REG, Timer8Main_INT_MASK
   endm

   macro Timer8Main_DisableInt_M
   M8C_DisableIntMask Timer8Main_INT_REG, Timer8Main_INT_MASK
   endm


